#HAZ UNA CALCULADORA """"
61
num1 = int(input("numero1: "))
num2 = int(input("numero2: "))
operacion = input("introduce una operacion (+ - * /)")

match operacion:
    case "+":
        res = num1+num2
    case "-":
        res = num1-num2
    case "*":
        res = num1*num2
    case "/":
        res = num1/num2

print(f"El resultado de {num1} {operacion} {num2} = {res}")
